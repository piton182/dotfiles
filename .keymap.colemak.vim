nmap <leader>h <C-w>h
nmap <leader>n <C-w>j
nmap <leader>e <C-w>k
nmap <leader>i <C-w>l
nmap <leader>, <C-w><C-w>

" enter INSERT mode
noremap u i
" go to the beginning of the line and enter INSERT mode
noremap U I
" undo last
noremap l u
noremap f e
noremap n j
noremap e k
noremap i l
" noremap j <nop>
" noremap k <nop>
